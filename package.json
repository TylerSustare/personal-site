{
  "name": "personal-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "next build",
    "start": "next start",
    "lint": "NODE_ENV=development eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "check-types": "NODE_ENV=development tsc",
    "prettier": "NODE_ENV=development prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "format": "NODE_ENV=development npm run prettier -- --write",
    "check-format": "NODE_ENV=development npm run prettier -- --list-different",
    "validate": "NODE_ENV=development npm-run-all --parallel check-types check-format lint build"
  },
  "dependencies": {
    "apollo-server-micro": "^2.15.0",
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.38",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.0",
    "graphql": "^15.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  }
}
